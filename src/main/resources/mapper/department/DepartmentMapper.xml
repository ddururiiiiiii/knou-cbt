<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="knou.cbt.domain.department.DepartmentMapper">

    <select id="allSearchDepartmentList" resultType="knou.cbt.domain.department.Department">
        SELECT DEPARTMENT_ID
             , DEPARTMENT_NAME
        FROM DEPARTMENT
        WHERE USE_YN = 'Y'
    </select>

</mapper>